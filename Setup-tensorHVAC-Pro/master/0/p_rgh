FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{

    floor
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           $internalField;
    }

    ceiling
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           $internalField;
    }

    inlet_1
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           $internalField;
    }

    outlet
    {
        type            fixedValue;
        value           $internalField;
    }

    wall
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           $internalField;
    }

    object_1
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           $internalField;
    }

    inlet_1
    {
        type            fixedFluxPressure;
        gradient        uniform 0;
        value           $internalField;
    }

    wind
    {
        type            fixedValue;
        value           $internalField;
    }
}


