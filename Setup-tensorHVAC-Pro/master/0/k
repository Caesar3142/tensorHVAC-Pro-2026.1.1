FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.001350;

boundaryField
{

    floor
    {
        type            kqRWallFunction;
        value           $internalField;
    }

    ceiling
    {
        type            kqRWallFunction;
        value           $internalField;
    }

    inlet_1
    {
        type            fixedValue;
        value           $internalField;
    }

    wind
    {
        type            fixedValue;
        value           uniform 0.003;
    }


    outlet
    {
        type            zeroGradient;
    }

    wall
    {
        type            kqRWallFunction;
        value           $internalField;
    }

    object_1
    {
        type            kqRWallFunction;
        value           $internalField;
    }
}


